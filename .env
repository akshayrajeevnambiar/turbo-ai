# Turbo AI Environment Variables

# Analytics Configuration
# Set to 'true' to enable analytics tracking
VITE_ENABLE_ANALYTICS=false

# Analytics Provider ('google' or leave empty for no-op)
VITE_ANALYTICS_PROVIDER=google

# Google Analytics 4 Measurement ID
# Get this from your GA4 property settings
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Development flags
VITE_DEBUG=false
VITE_LOG_LEVEL=warn

# Feature flags
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true

# Base URL
VITE_BASE_URL=http://localhost:4173
